{
  "name": "ellis",
  "version": "0.0.1",
  "description": "Modulor directory listings",
  "main": "index.js",
  "dependencies": {
    "yargs": "5.0.0"
  },
  "scripts": {
    "start": "xo & chokidar ./ -c './index.js -t -p \"date-dots filenames\" node_modules'"
  },
  "bin": {
    "l": "./index.js"
  },
  "author": "Christopher Newton (https://github.com/chrstphrknwtn)",
  "license": "MIT",
  "devDependencies": {
    "chokidar-cli": "1.2.0",
    "xo": "0.16.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chrstphrknwtn/ellis.git"
  },
  "xo": {
    "semicolon": false,
    "space": 2,
    "rules": {
      "no-warning-comments": 0,
      "xo/no-process-exit": 0
    }
  }
}
